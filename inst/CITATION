citHeader("To cite eapath in publications, please use:")

citEntry(entry = "Article",
         author = personList(as.person("Nicole C. Kleinstreuer"),
                             as.person("Patricia Cegar"),
                             as.person("Eric D. Watt"),
                             as.person("Matthew T. Martin"),
                             as.person("Keith A. Houck"),
                             as.person("Patience Browne"),
                             as.person("Russell S. Thomas"),
                             as.person("Warren Casey"),
                             as.person("David Dix"),
                             as.person("David Allen"),
                             as.person("Srilatha Sakamuru"),
                             as.person("Menghang Xia"),
                             as.person("Ruili Huang"),
                             as.person("Richard S. Judson")),
         title = "Development and Validation of a Computational Model for Androgen Receptor Activity",
         journal = "Chemical Research in Toxicology",
         year = "2016",
         doi = "10.1021/acs.chemrestox.6b00347",
         url = "http://pubs.acs.org/doi/abs/10.1021/acs.chemrestox.6b00347",
         textVersion = paste('Kleinstreuer NC, Cegar P, Watt ED, Martin MT, Houck KA, Browne P, Thomas RS,',
                             'Casey W, Dix D, Allen D, Sakamuru S, Xia M, Huang R and Judson RS (2016).',
                             '"Development and Validation of a Computational Model for Androgen Receptor Activity."',
                             'Chemical Research in Toxicology.',
                             'URL: http://pubs.acs.org/doi/abs/10.1021/acs.chemrestox.6b00347.')
)

